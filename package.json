{
  "name": "mc-ui-comatv",
  "version": "1.1.2",
  "type": "module",
  "description": "A collection of Minecraft-styled React UI components with pixelated design, including custom scrollbars, adaptable buttons, and grid layouts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./styles": "./dist/styles.css",
    "./fonts": {
      "MinecraftRegular": "./dist/MinecraftRegular.ttf",
      "MinecraftTen": "./dist/MinecraftTen.ttf"
    }
  },
  "files": [
    "dist",
    "README.md",
    "TROUBLESHOOTING.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:lib",
    "build:lib": "rollup -c",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "components",
    "minecraft",
    "ui",
    "pixelated",
    "game-ui",
    "react-components",
    "scrollbar",
    "grid-layout",
    "custom-buttons",
    "dark-theme"
  ],
  "author": "Your Name",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@tailwindcss/vite": "^4.1.11",
    "classnames": "^2.5.1",
    "framer-motion": "^12.23.3",
    "minecraft-ui-components": "^1.0.0",
    "react-use-gesture": "^9.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.25.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "fs-extra": "^11.3.0",
    "globals": "^16.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "rollup": "^4.46.0",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ComaTV/Minecraft-UI-Components-.git"
  },
  "bugs": {
    "url": "https://github.com/ComaTV/Minecraft-UI-Components-/issues"
  },
  "homepage": "https://github.com/ComaTV/Minecraft-UI-Components-#readme"
}
